<!DOCTYPE html>
<html>

<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <style>
        body {
            font-size: 10px;
        }

        table,
        th,
        td {
            border: 1px solid black;
            border-collapse: collapse;
        }

        .header {
            height: 300px;
            background-color: aqua;
            margin-bottom: 20px;
        }
    </style>

    <head>

    <body>
        <div class="report-body">
            <div class="info">
                <!-- <br /> -->
                <table class="table table-striped mt-10" cellpadding="2">
                    <thead>
                        <tr class="table-active">
                            <th width="5%" scope="col">Sr.No.</th>
                            <% (data.column).forEach(function(user,i) { %>
                                <th width="10%" scope="col">
                                    <%= user.label %>
                                </th>
                                <% }); %>
                        </tr>
                    </thead>
                    <tbody>
                        <% (data.list).forEach(function(user,i) { %>
                            <tr>
                                <td>
                                    <%= i+1 %>.
                                </td>
                                <td>
                                    <%= user.scheme_name %>
                                </td>
                                <td>
                                    <%= user.major_component_name %>
                                </td>
                                <td>
                                    <%= user.sub_component_name %>
                                </td>
                                <td>
                                    <%= user.activity_master_name %>
                                </td>
                                <td>
                                    <%= user.activity_master_details_name %>
                                </td>
                                <td>
                                    <%= user.recuring_nonrecuring %>
                                </td>
                            </tr>
                            <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </body>

</html>