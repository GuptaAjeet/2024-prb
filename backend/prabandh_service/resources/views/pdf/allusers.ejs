<!DOCTYPE html>
<html>

<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <style>
        body {
            font-size: 10px;
        }

        table,
        th,
        td {
            border: 1px solid black;
            border-collapse: collapse;
        }

        .header {
            height: 300px;
            background-color: aqua;
            margin-bottom: 20px;
        }
    </style>

    <head>

    <body>
        <div class="report-body">
            <div class="info">
                <table class="table table-striped mt-10" cellpadding="2">
                    <thead>
                        <tr class="table-active">
                            <th width="5%" scope="col">Sr.No.</th>
                            <th width="15%" scope="col">Name</th>
                            <!-- <% if(data.district_id==0) { %> -->
                            <!-- <th width="15%" scope="col">Major Component</th> -->
                            <!-- <% } %> -->
                            <th width="10%" scope="col">Mobile</th>
                            <th width="15%" scope="col">Email</th>
                            <th width="20%" scope="col">Role</th>
                            <th width="10%" scope="col">District Name</th>
                            <th width="10%" scope="col">State Name</th>
                            <th width="5%" scope="col">Status</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% (data.list).forEach(function(user,i) { %>
                            <tr>
                                <td>
                                    <%= i+1 %>.
                                </td>
                                <td>
                                    <%= user.user_name %>
                                </td>
                                <td>
                                    <%= user.user_mobile %>
                                </td>
                                <td>
                                    <%= user.user_email %>
                                </td>
                                <td>
                                    <%= user.role_name %>
                                </td>
                                <td>
                                    <%= user.district_name %>
                                </td>
                                <td>
                                    <%= user.state_name %>
                                </td>
                                <td>
                                    <%= user.user_status ? "Active" :"Inactive" %>
                                </td>
                            </tr>
                            <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </body>

</html>