<!DOCTYPE html>
<html>

<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <style>
        body {
            font-size: 10px;
        }

        table,
        th,
        td {
            border: 1px solid black;
        }

        .header {
            height: 300px;
            background-color: aqua;
            margin-bottom: 20px;
        }
    </style>

    <head>

    <body>
        <div class="report-body">
            <div class="info">
                <!-- <br /> -->
                <table class="table table-striped mt-10" cellpadding="2" border="1">
                    <thead>
                        <tr class="table-active">
                            <th width="5%" scope="col">Sr.No.</th>
                            <th width="10%" scope="col">Scheme</th>
                            <th width="10%" scope="col">Major Component</th>
                            <th width="15%" scope="col">Sub Component</th>
                            <th width="10%" scope="col">Activity</th>
                            <th width="20%" scope="col">Sub Activity</th>
                            <th width="5%" scope="col">Physical</th>
                            <th width="5%" scope="col">Unit Cost (In Lakhs)</th>
                            <th width="5%" scope="col">Financial (In Lakhs)</th>
                        </tr>
                    </thead>
                    <tbody border="1">
                        <% (data.list).forEach(function(user,i) { %>
                            <tr>
                                <td>
                                    <%= i+1 %>.
                                </td>
                                <td>
                                    <%= user.scheme_name %>
                                </td>
                                <!-- <% if(data.district_id==0) { %>
                                    <td>
                                        <%=user.district_id %>
                                    </td>
                                    <% } %> -->
                                <td>
                                    <%= user.major_component_name %>
                                </td>
                                <td>
                                    <%= user.sub_component_name %>
                                </td>
                                <td>
                                    <%= user.activity_master_name %>
                                </td>
                                <td>
                                    <%= user.activity_master_details_name %>
                                </td>
                                <td style="text-align: right;">
                                    <%= user.physical_quantity %>
                                </td>
                                <td style="text-align: right;">
                                    <%= user.unit_cost %>
                                </td>
                                <td style="text-align: right;">
                                    <%= user.financial_amount %>
                                </td>
                                <!-- <td>
                                    <% if(user.status==1) { %> Active <% } else { %>Inactive <% } %>
                                </td> -->
                            </tr>
                            <% }); %>
                            <tr>
                                <td  colspan="6" style="text-align: right;font-weight: bold;">
                                    Total(₹ In Lakhs) :
                                </td>
                                <td style="text-align: right;font-weight: bold;">
                                    <%= data?.quantity %>
                               </td>
                               <td style="text-align: center;font-weight: bold;">
                                ----
                                </td>
                                <td style="font-weight: bold;">
                                    ₹ <%= data?.financial_amount %>
                                </td>
                            </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </body>

</html>